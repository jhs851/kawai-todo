{
  "expo": {
    "name": "Kawai To Do",
    "slug": "kawai-todo",
    "description": "Kawai To Do App made with React Native",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#f23657"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "kr.hyungseok.kawai"
    },
    "android": {
      "package": "kr.hyungseok.kawai"
    },
    "githubUrl": "https://github.com/jhs851/kawai-todo"
  }
}